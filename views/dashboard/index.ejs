<div class="pt-40 flex flex-col gap-8 items-center">
  <h2>Recently Added</h2>
  <div>
    <% mascots.forEach(mascot => { %>
    <div><%= mascot.name %></div>
    <a href="/mascots/<%= mascot.id %>">
      <img height="150" width="150" src="<%= mascot.imagePath %>" />
    </a>
    <% }) %>
  </div>
</div>
